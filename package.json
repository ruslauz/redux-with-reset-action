{
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "pub": "npm publish --access public",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "redux": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@ruslanuz/redux-with-reset-action",
  "author": "Sergei",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruslauz/redux-with-reset-action.git"
  },
  "homepage": "https://github.com/ruslauz/redux-with-reset-action",
  "keywords": [
    "redux",
    "reset"
  ],
  "module": "dist/redux-with-reset-action.esm.js",
  "size-limit": [
    {
      "path": "dist/redux-with-reset-action.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/redux-with-reset-action.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.4",
    "redux": "^4.1.2",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "semantic-release": "^19.0.2 --branches main"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}